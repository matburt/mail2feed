{
  "name": "mail2feed-frontend",
  "version": "1.0.0",
  "description": "Frontend for mail2feed - converts mailing lists to RSS/Atom feeds",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
